{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "Distribution of the speed limits at each crash location by states",
  "width": 500,
  "height": 300,
  "data": { "url": "ardd_fatal_crashes.csv" },
  "mark": "boxplot",
  "encoding": {
    "x": { "field": "State_Full", "type": "nominal", "title": "States" },
    "y": {
      "field": "Speed Limit",
      "type": "quantitative",
      "title": "Speed Limit (km/h)"
    },
    "color": { "field": "State_Full", "type": "nominal", "title": "States" }
  },
  "title": "Distribution of the speed limits at each crash location by states"
}
